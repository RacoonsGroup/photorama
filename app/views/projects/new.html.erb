<h2>Projects</h2>
  <div><%= form_for(:project, url: projects_path ) do |f| %>
    <div><%= f.label :title %><br />
    <%= f.text_field :title %></div>

    <div><%= f.label :subdomain %><br />
    <%= f.text_field :subdomain %></div>
  
    <div><%= f.submit "Save params" %></div>
  <% end %>
  </div>
